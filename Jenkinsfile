pipeline {

  agent any
    
  stages {
    
    stage('Install Npm') {
       steps {
         sh 'npm install'
       }
    }

    stage('Instalar NPM') {
       steps {
         sh 'npm install'
       }
    }
    
    stage('ABRIR O PROJETO') {
       steps {
         sh 'npm start --open --port:7500'
       }
    }

  }
}
