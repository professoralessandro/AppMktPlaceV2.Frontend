pipeline {

  agent any
    
  stages {
    
    stage('Install Npm') {
       steps {
         bat 'npm install'
       }
    }

    stage('Instalar NPM') {
       steps {
         bat 'npm install'
       }
    }
    
    stage('ABRIR O PROJETO') {
       steps {
         bat 'npm start'
       }
    }

  }
}
